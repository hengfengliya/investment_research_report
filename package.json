  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0-rc.12",
    "drizzle-orm": "^0.33.0",
    "hono": "^4.5.3",
    "p-limit": "^5.0.0",
    "prisma": "^5.20.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vercel/node": "^5.5.3",
    "typescript": "^5.5.4"
  }
}
